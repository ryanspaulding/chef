
Host sfe
	User rspauldi
	Hostname sfe2.nas.nasa.gov
	ControlPath ~/.ssh/%r-%h-%p
	ControlMaster auto

# change this to bruiser if bouncer is down
Host bouncer
	User rspauldi
	HostName    bouncer.nas.nasa.gov
   	ControlPath ~/.ssh/%r-%h-%p
	ControlMaster auto

Host seven
	User rspauldi
	HostName seven.nas.nasa.gov
    	ControlPath     ~/.ssh/%r-%h-%p
    	ProxyCommand    ssh bouncer /usr/local/bin/ssh-proxy %h
	ControlMaster auto

