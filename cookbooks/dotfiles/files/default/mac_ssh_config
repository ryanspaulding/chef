Host sfe sfe?.nas.nasa.gov dmzfs?.nas.nasa.gov sup*.nas.nasa.gov
	User rspauldi
	LogLevel                info
	ProxyCommand            none


Host lfe?.nas.nasa.gov 
	User rspauldi
	ForwardAgent yes
	ProxyCommand ssh sfe /usr/local/bin/ssh-proxy %h
	ControlPath     ~/.ssh/%r-%h-%p
	ControlMaster auto
